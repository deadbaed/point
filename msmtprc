# Set default values for all following accounts.
defaults
auth           on
tls            on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile        ~/.log.msmtp

# Gmail
account        gmail
host           smtp.gmail.com
port           587
from           loctauxphilippe@gmail.com
user           loctauxphilippe
password       # password

# Set a default account
account default : gmail
