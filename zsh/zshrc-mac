# Homebrew for macOS
eval "$(/opt/homebrew/bin/brew shellenv)"

if type brew &>/dev/null; then
  FPATH=$(brew --prefix)/share/zsh/site-functions:$FPATH

  autoload -Uz compinit
  compinit
fi

export JAVA_HOME="/Users/phil/Library/Java/JavaVirtualMachines/liberica-1.8.0_302"
export ANDROID_TOOLS="/Users/phil/Library/Android/sdk/platform-tools"
# export JAVA_HOME=$(/usr/libexec/java_home)
export PATH="$JAVA_HOME/bin:$ANDROID_TOOLS:/Users/phil/go/bin:$PATH"
