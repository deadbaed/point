# zsh and ohmyzsh
export ZSH=$HOME/.oh-my-zsh
export ZSH_CUSTOM=$HOME/point/zsh/

# path
export PATH="$HOME/bin:$HOME/.local/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:$HOME/.cargo/bin:$PATH"

# env
export LANG=en_US.UTF-8
export TERM=xterm-256color # for osx
export EDITOR='vim'
export SSH_KEY_PATH="~/.ssh/id_rsa"
export GPG_TTY=$(tty)
